{
    "name": "ivkeyminds/invoice-ninja-client",
    "description": "Invoice Ninja PHP Client generated from OpenAPI specification.",
    "keywords": [
        "invoice-ninja",
        "api-client",
        "php",
        "sdk",
        "openapi"
    ],
    "homepage": "https://github.com/ivkeyminds/invoice-ninja-client",
    "license": "MIT",
    "require": {
        "php": "^7.1 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/psr7": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "autoload": {
        "psr-4": {
            "IvkeyMinds\\InvoiceNinjaClient\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IvkeyMinds\\InvoiceNinjaClient\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.5.x-dev"
        }
    }
}
